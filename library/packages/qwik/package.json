    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.51.0",
    "eslint-plugin-qwik": "latest",
    "np": "^8.0.4",
    "postcss": "^8.4.38",
    "tailwindcss": "3.4.3",
    "typescript": "^5.3.2",
    "undici": "^5.26.0",
    "vite": "^4.4.11",
    "vite-tsconfig-paths": "^4.2.1"
